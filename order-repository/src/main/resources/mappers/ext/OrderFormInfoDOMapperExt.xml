<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cdu.lys.graduation.repository.dao.ext.OrderFormInfoDOMapperExt" >

    <select id="selectByOrderNum" resultMap="com.cdu.lys.graduation.repository.dao.OrderFormInfoDOMapper.BaseResultMap">
        SELECT
        <include refid="com.cdu.lys.graduation.repository.dao.OrderFormInfoDOMapper.Base_Column_List"></include>
        FROM tb_order_form_info
        WHERE order_num=#{orderNum}
    </select>
</mapper>